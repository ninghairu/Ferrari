#include<iostream>
using namespace std;
int main()
{
	int i,j,h,l,k=0,a[8][8]={{0},{0},{0}},n,flag=0;
	char x;
	
	for(i=0;i<8;i++)
	{
		cin>>x;
		if(x=='*') {k=i;continue;}
		if(x!='*')
		{
		n=x-'0';
		l=n-1;h=i;
		for(j=0;j<8;j++)	
			a[i][j]=1;
		for(j=0;j<8;j++)	
			a[j][l]=1;
			
		for(j=l-1;j>=0;j--)
			if(h+1<8)	a[++h][j]=1;
		h=i;
		for(j=l-1;j>=0;j--)
			if(h-1>=0)	a[--h][j]=1;
		h=i;
		for(j=l+1;j<8;j++)
			if(h+1<8)	a[++h][j]=1;
		h=i;
		for(j=l+1;j<8;j++)
			if(h-1>=0)	a[--h][j]=1;
		}
	}
	
	for(j=0;j<8;j++)
		if(a[k][j]==0) {n=j+1;flag=1;break;}
	if(flag==1)	cout<<n<<endl;
	else cout<<"No Answer"<<endl;
	return 0;
}
